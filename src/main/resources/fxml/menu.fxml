<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="720.0" prefWidth="1000.0" styleClass="bg-light-blue" stylesheets="@../css/stylesheet.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.MenuController">
   <children>
      <Label alignment="CENTER" layoutX="82.0" prefHeight="117.0" prefWidth="852.0" stylesheets="@../css/stylesheet.css" text="Speedy Sketchers presents:">
         <font>
            <Font name="Bauhaus 93" size="38.0" />
         </font>
         <styleClass>
            <String fx:value="text-purple" />
            <String fx:value="header-text" />
         </styleClass>
      </Label>
      <Label fx:id="headerLabel" alignment="CENTER" layoutX="142.0" layoutY="198.0" prefHeight="123.0" prefWidth="764.0" styleClass="text-purple" text="Whos Playing?">
         <font>
            <Font name="Segoe UI Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="addProfileButton" layoutX="477.0" layoutY="453.0" mnemonicParsing="false" onAction="#onAdd" prefHeight="60.0" prefWidth="60.0" text="+" textAlignment="CENTER">
         <styleClass>
            <String fx:value="bg-light-purple" />
            <String fx:value="text-white" />
            <String fx:value="new-profile" />
            <String fx:value="rounded" />
         </styleClass>
         <font>
            <Font name="System Bold" size="40.0" />
         </font></Button>
      <HBox fx:id="profilesHbox" alignment="CENTER" layoutX="56.0" layoutY="285.0" prefHeight="100.0" prefWidth="903.0" spacing="10.0">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding></HBox>
      <Button fx:id="switchProfileButton" layoutX="14.0" layoutY="606.0" mnemonicParsing="false" onAction="#onSwitchProfile" prefHeight="100.0" prefWidth="100.0">
         <styleClass>
            <String fx:value="bg-purple" />
            <String fx:value="rounded" />
         </styleClass>
         <graphic>
            <ImageView fitHeight="72.0" fitWidth="84.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/users.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="displayStatsButton" layoutX="886.0" layoutY="606.0" mnemonicParsing="false" onAction="#onDisplayStats" prefHeight="100.0" prefWidth="100.0">
         <graphic>
            <ImageView fitHeight="76.0" fitWidth="78.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/trophy.png" />
               </image>
            </ImageView>
         </graphic>
         <styleClass>
            <String fx:value="bg-purple" />
            <String fx:value="rounded" />
         </styleClass></Button>
      <Button fx:id="classicButton" layoutX="425.0" layoutY="385.0" mnemonicParsing="false" onAction="#onStartClassic" text="Classic">
         <font>
            <Font name="Segoe UI" size="39.0" />
         </font>
         <styleClass>
            <String fx:value="bg-purple" />
            <String fx:value="text-white" />
            <String fx:value="rounded" />
         </styleClass>
      </Button>
      <Label alignment="CENTER" layoutX="41.0" layoutY="81.0" prefHeight="117.0" prefWidth="918.0" stylesheets="@../css/stylesheet.css" text="Quick, Draw! SE206  Edition">
         <font>
            <Font name="Bauhaus 93" size="65.0" />
         </font>
         <styleClass>
            <String fx:value="text-purple" />
            <String fx:value="header-text" />
         </styleClass>
      </Label>
      <Button fx:id="zenButton" layoutX="255.0" layoutY="385.0" mnemonicParsing="false" onAction="#onStartZen" text="Zen">
         <font>
            <Font name="Segoe UI" size="39.0" />
         </font>
         <styleClass>
            <String fx:value="bg-purple" />
            <String fx:value="text-white" />
            <String fx:value="rounded" />
         </styleClass>
      </Button>
      <Button fx:id="hiddenButton" layoutX="627.0" layoutY="385.0" mnemonicParsing="false" onAction="#onStartHidden" text="Hidden">
         <font>
            <Font name="Segoe UI" size="39.0" />
         </font>
         <styleClass>
            <String fx:value="bg-purple" />
            <String fx:value="text-white" />
            <String fx:value="rounded" />
         </styleClass>
      </Button>
   </children>
</Pane>
