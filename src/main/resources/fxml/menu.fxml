<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="720.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.MenuController">
   <children>
      <Label layoutX="353.0" layoutY="150.0" prefWidth="294.0" text="Speedy Sketchers">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Button fx:id="startButton" layoutX="451.0" layoutY="475.0" mnemonicParsing="false" onAction="#onSwitchToReady" styleClass="red" text="Start">
         <font>
            <Font size="29.0" />
         </font>
      </Button>
      <Label fx:id="headerLabel" alignment="CENTER" layoutX="353.0" layoutY="238.0" prefHeight="17.0" prefWidth="294.0" text="Whos Playing?">
         <font>
            <Font name="SansSerif Regular" size="22.0" />
         </font>
      </Label>
      <Button fx:id="addProfileButton" layoutX="477.0" layoutY="453.0" mnemonicParsing="false" onAction="#onAdd" prefHeight="43.0" prefWidth="47.0" text="+" />
      <HBox fx:id="profilesHbox" alignment="CENTER" layoutX="229.0" layoutY="321.0" prefHeight="100.0" prefWidth="541.0" spacing="10.0">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding></HBox>
      <Button fx:id="switchProfileButton" layoutX="14.0" layoutY="651.0" mnemonicParsing="false" onAction="#onSwitchProfile" prefHeight="55.0" prefWidth="108.0" text="SwitchButton" />
      <Button fx:id="displayStatsButton" layoutX="878.0" layoutY="651.0" mnemonicParsing="false" onAction="#onDisplayStats" prefHeight="55.0" prefWidth="108.0" text="stats" />
   </children>
</Pane>
